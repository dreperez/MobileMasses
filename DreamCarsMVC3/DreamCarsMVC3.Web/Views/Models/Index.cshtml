@model IEnumerable<DreamCarsMVC3.Models.Model>

@{
    ViewBag.Title = "Models";
}

<h2>Models</h2>

<ul>
    @foreach (var item in Model) {
        <li>
            @Html.ActionLink(string.Format("{0} {1} {2}", @item.Year, @item.Manufacturer.Name, @item.Name), "Details", "Models", new { id = item.ModelId }, null)
        </li>
    }
</ul>




